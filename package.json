{
  "name": "rollup-plugin-drop-console",
  "version": "0.0.0",
  "description": "A rollup plugin used to drop console",
  "author": "chouchouji <1305974212@qq.com>",
  "license": "MIT",
  "type": "module",
  "module": "lib/index.js",
  "main": "lib/index.cjs",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm --out-dir=lib --watch --dts",
    "build": "tsup src/index.ts --format esm,cjs --out-dir=lib --dts --clean",
    "format": "prettier --write ."
  },
  "files": [
    "lib",
    "tsconfig.json"
  ],
  "keywords": [
    "rollup-plugin",
    "transform",
    "console"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chouchouji/rollup-plugin-drop-console.git"
  },
  "bugs": {
    "url": "https://github.com/chouchouji/rollup-plugin-drop-console/issues"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.11"
  },
  "devDependencies": {
    "@varlet/release": "^0.2.11",
    "prettier": "^3.3.3",
    "rollup": "^4.21.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}